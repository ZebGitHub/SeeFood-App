{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
      "esModuleInterop": true,
      "strict": true,
      "allowJs": true,   
      "jsx": "react-native",
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": [
      "**/*.ts",
      "**/*.tsx",
      ".expo/types/**/*.ts",
      "expo-env.d.ts"
    ]
  }
